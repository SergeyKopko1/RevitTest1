<UserControl x:Class="RevitTest.View.Controls.RunButtonControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RevitTest.View.Controls"
             xmlns:conv="clr-namespace:RevitTest.Converter"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <Grid>
            <Button Content="Выполнить" 
                Command="{Binding ChangeWindowSizeCommand}"
                Height="29" Width="360" 
                VerticalAlignment="Bottom" 
                Margin="0 0 0 20" 
            />
    </Grid>
</UserControl>
